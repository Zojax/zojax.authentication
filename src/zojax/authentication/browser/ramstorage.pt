<div class="topframe" i18n:domain="zojax.authentication">
  <h1 tal:content="context/__title__">PrincipalInfo storage</h1>
  <div tal:content="context/__description__"></div>
  <br />

  <div class="z-table-container">
    <table class="z-table">
      <thead>
	<th i18n:translate="">Path</th>
	<th i18n:translate="">Hits</th>
	<th i18n:translate="">Misses</th>
      </thead>
      <tbody>
	<tr tal:repeat="data view/getStatistics">
          <td><span tal:content="data/path">&nbsp;</span></td>
          <td><span tal:content="data/hits">&nbsp;</span></td>
          <td><span tal:content="data/misses">&nbsp;</span></td>
	</tr>
      </tbody>
    </table>
  </div>
</div>
